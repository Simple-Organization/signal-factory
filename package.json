{
  "name": "signal-factory",
  "version": "1.0.0-next.4",
  "author": "Simple Organization",
  "scripts": {
    "build": "tsc && rollup -c && node esbuild.mjs",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "playwright test",
    "prepack": "pnpm build",
    "benchmark": "tsx tests/benchmark.ts"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simple-Organization/signal-factory.git"
  },
  "bugs": "https://github.com/Simple-Organization/signal-factory/issues",
  "license": "MIT",
  "devDependencies": {
    "@angular/core": ">=16",
    "@playwright/test": "^1.46.0",
    "@types/benchmark": "^2.1.5",
    "benchmark": "^2.1.4",
    "esbuild": "^0.23.0",
    "preact": ">=10",
    "prettier": "^3.3.3",
    "rollup": "^4.20.0",
    "rollup-plugin-dts": "^6.1.1",
    "solid-js": ">=1.8",
    "svelte": "^4.2.18",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vue": ">=3"
  },
  "files": [
    "dist/index.d.ts",
    "dist/index.js",
    "vue/index.d.ts",
    "vue/index.js",
    "solid/index.d.ts",
    "solid/index.js",
    "angular/index.d.ts",
    "angular/index.js",
    "preact/index.d.ts",
    "preact/index.js"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js",
      "browser": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./vue": {
      "types": "./vue/index.d.ts",
      "default": "./vue/index.js",
      "browser": "./vue/index.js",
      "import": "./vue/index.js"
    },
    "./solid": {
      "types": "./solid/index.d.ts",
      "default": "./solid/index.js",
      "browser": "./solid/index.js",
      "import": "./solid/index.js"
    },
    "./angular": {
      "types": "./angular/index.d.ts",
      "default": "./angular/index.js",
      "browser": "./angular/index.js",
      "import": "./angular/index.js"
    },
    "./testing": {
      "types": "./testing/index.d.ts",
      "default": "./testing/index.js",
      "browser": "./testing/index.js",
      "import": "./testing/index.js"
    },
    "./preact": {
      "types": "./preact/index.d.ts",
      "default": "./preact/index.js",
      "browser": "./preact/index.js",
      "import": "./preact/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "type": "module",
  "packageManager": "pnpm@9.6.0"
}
